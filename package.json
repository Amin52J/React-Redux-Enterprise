{
  "name": "react-redux-enterprise",
  "version": "1.5.0",
  "author": "Amin Jafari",
  "description": "A React-Redux boilerplate for enterprise web applications",
  "main": "app/main.jsx",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production",
    "dev": "webpack --mode development",
    "docs": "documentation build app/** -f html -o docs",
    "lint": "eslint ./app/**/*.{js,jsx}",
    "prebuild": "rm -rf public",
    "precommit": "lint-staged",
    "predocs": "rm -rf docs",
    "preversion": "yarn test && yarn build && yarn docs",
    "start": "http-server-spa ./public",
    "test": "jest --updateSnapshot",
    "watch": "webpack --mode development --watch"
  },
  "browserslist": [
    "> 5%"
  ],
  "jest": {
    "moduleNameMapper": {
      "^@actions(.*)$": "<rootDir>/app/actions$1",
      "^@components(.*)$": "<rootDir>/app/components$1",
      "^@constants(.*)$": "<rootDir>/app/constants$1",
      "^@containers(.*)$": "<rootDir>/app/containers$1",
      "^@elements(.*)$": "<rootDir>/app/elements$1",
      "^@hoc(.*)$": "<rootDir>/app/hoc$1",
      "^@reducers(.*)$": "<rootDir>/app/reducers$1",
      "^@epics(.*)$": "<rootDir>/app/epics$1",
      "^@root(.*)$": "<rootDir>/app$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/shim.js",
      "<rootDir>/test.setup.js"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      [
        "env"
      ]
    ],
    "plugins": [
      "transform-es2015-destructuring",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^4.0.0",
    "redux-observable": "^0.18.0",
    "redux-persist": "^5.9.1",
    "rxjs": "^5.5.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8",
    "babel-jest": "^23.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "path": "^0.12.7",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.4.0",
    "sass-loader": "^7.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4"
  },
  "lint-staged": {
    "*.{md,json,css,scss,js,jsx}": [
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "repository": "https://github.com/Amin52J/React-Redux-Enterprise",
  "license": "MIT",
  "private": false
}
